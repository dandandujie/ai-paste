{
  "name": "ai-paste",
  "version": "1.0.0",
  "description": "Browser extension to copy AI chat content with proper formatting for Word/Excel/WPS",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && vite build --config vite.config.content.ts",
    "build:main": "vite build",
    "build:content": "vite build --config vite.config.content.ts",
    "preview": "vite preview"
  },
  "dependencies": {
    "@rollup/rollup-darwin-arm64": "^4.53.3",
    "highlight.js": "^11.9.0",
    "katex": "^0.16.25",
    "marked": "^12.0.0",
    "mathml2omml": "^0.5.0"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.260",
    "rollup": "^4.53.3",
    "terser": "^5.44.1",
    "typescript": "^5.3.3",
    "vite": "^5.4.21",
    "vite-plugin-static-copy": "^1.0.0"
  },
  "optionalDependencies": {
    "@rollup/rollup-win32-x64-msvc": "^4.24.4"
  }
}
